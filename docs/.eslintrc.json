{
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "settings": {
        "import/resolver": {
          "typescript": {
            "alwaysTryTypes": true,
            "project": ["tsconfig.json", "docs/tsconfig.json"]
          }
        }
      },
      "rules": {
        "@typescript-eslint/no-non-null-assertion": "off",
        "import/no-unresolved": [2, { "ignore": ["virtual:uno.css"] }]
      }
    }
  ]
}
